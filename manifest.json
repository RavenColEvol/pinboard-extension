{
    "manifest_version": 2,
    "name": "Pin Board",
    "version": "0.1",
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png"
    },
    "browser_action": {
        "default_icon": "icons/icon-16.png",
        "default_title": "Pin Board",
        "default_popup": "pinboard.html"
    },
    "permissions": [
        "storage"
    ]
}